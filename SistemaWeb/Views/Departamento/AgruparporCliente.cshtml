@model List<Model.Entity.Moldes.ClienteEntity>

@{
    ViewBag.Title = "AgruparporCliente";
}

<h2 class="text-center text-success">Cliente por Departamento</h2>

<table class="table table-hover table-striped">
    <thead>
        <tr class="bg-primary">
            <th>Código</th>
            <th>Nombre</th>

            <th colspan="3">Operaciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var lista in Model)
        {
            <tr>
                <td>@lista.IdCliente</td>
                <td>@lista.NombreCompleto</td>
              

            </tr>
        }
    </tbody>

</table>